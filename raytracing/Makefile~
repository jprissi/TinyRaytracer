raytracer: camera.o vector.o main.cpp
	g++ -Wall -Wextra -Wpedantic -std=c++11 -O2 main.cpp -o raytracer
camera.o: camera.cpp camera.h
	g++ -Wall -Wextra -Wpedantic -std=c++11 -O2 camera.cpp -o camera.o
vector.o: vector.cpp vector.h
	g++ -Wall -Wextra -Wpedantic -std=c++11 -O2 vector.cpp -o vector.o
